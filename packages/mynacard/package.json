{
  "name": "@aokiapp/mynacard",
  "version": "0.0.1",
  "description": "Japanese MynaCard (マイナンバーカード) support with JPKI, Kenhojo, and Kenkaku applications",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist .turbo node_modules"
  },
  "keywords": [
    "mynacard",
    "マイナンバーカード",
    "japan",
    "jpki",
    "kenhojo",
    "kenkaku",
    "government-id",
    "smartcard"
  ],
  "author": "株式会社AokiApp (AokiApp Inc.) <https://aoki.app>",
  "license": "ANAL",
  "homepage": "https://github.com/AokiApp/jsapdu#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AokiApp/jsapdu.git",
    "directory": "packages/mynacard"
  },
  "bugs": {
    "url": "https://github.com/AokiApp/jsapdu/issues"
  },
  "dependencies": {
    "@aokiapp/tlv-parser": "^0.0.1"
  }
}