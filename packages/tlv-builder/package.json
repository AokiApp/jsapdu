{
  "name": "@aokiapp/tlv-builder",
  "version": "0.0.1",
  "description": "Tag-Length-Value (TLV) data builder with schema support for DER encoding",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "tsc --build --clean",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "tlv",
    "builder",
    "tag-length-value",
    "asn1",
    "der",
    "schema",
    "encoding",
    "smartcard"
  ],
  "author": "株式会社AokiApp (AokiApp Inc.) <https://aoki.app>",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/AokiApp/jsapdu#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/AokiApp/jsapdu.git",
    "directory": "packages/tlv-builder"
  },
  "bugs": {
    "url": "https://github.com/AokiApp/jsapdu/issues"
  },
  "devDependencies": {
    "vitest": "^2.0.0"
  }
}